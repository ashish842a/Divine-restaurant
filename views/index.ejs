<script>
   window.onscroll = function() {display()}; 
   function display() {
    var icon = document.getElementById("move_top");
    console.log(window.scrollY);
   if(window.scrollY > 500){
      icon.style.display = "initial";
  }
  else{
    icon.style.display = "none";
  }
}     
   function getBackToBeginning() { window.scrollTo(0, 0);
   }
    window.onload = function(){
            //hide the preloader
    setTimeout(function(){
        
        document.querySelector(".loader-pre").style.display = "none";
    },200)
}


    document.querySelector("#act").addEventListener("click",function(){
    console.log("click");
    document.querySelector(".bookTable").style.display="initial";

    })


    document.querySelector("#close-form").addEventListener("click",function(){
    console.log("click");
    document.querySelector(".bookTable").style.display="none";
    })

</script>


